{
  "name": "@webda/elasticsearch",
  "version": "0.0.1",
  "description": "Webda ElasticSearch implementation",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc && webda module --noCompile",
    "pretest": "npm run build",
    "test": "nyc mocha --timeout=100000",
    "run": "node core",
    "format": "node ./scripts/format.js",
    "precommit": "npm run format && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loopingz/webda.io.git"
  },
  "keywords": [
    "webda"
  ],
  "files": [
    "lib",
    "webda.module.json",
    "package.json"
  ],
  "author": "loopingz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/loopingz/webda.io/issues"
  },
  "homepage": "https://github.com/loopingz/webda-elasticsearch#readme",
  "dependencies": {
    "@webda/core": "0.11.3",
    "elasticsearch": "^15.1.1"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx",
      ".js"
    ],
    "include": [
      "lib/**/*.js"
    ],
    "reporter": [
      "html",
      "lcov",
      "json"
    ],
    "report-dir": "./reports",
    "all": true
  }
}
